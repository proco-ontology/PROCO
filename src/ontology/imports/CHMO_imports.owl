        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bridgman-Stockbarger technique</oboInOwl:hasExactSynonym>
        <oboInOwl:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CHMO:0002160</oboInOwl:id>
        <oboInOwl:created_by rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://orcid.org/0000-0001-5985-7429</oboInOwl:created_by>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/chmo.owl"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CHMO_0002198 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CHMO_0002198">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">recrystallisation</rdfs:label>
